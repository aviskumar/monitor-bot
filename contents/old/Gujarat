        function LoadDepartmentWise() {
            $.ajax({
                type: "POST",
                url: "DrillDownCharts.aspx/GetDistDataForCovid",
                contentType: "application/json; charset=utf-8",
                dataType: "json",

                success: function (response) {
                    var Data = response.d;
                    var newData = JSON.parse(response.d)
                    // var labels = [];
                    //var data = [];
                    //var JSData=[];
                    var name = Array();
                    var data = Array();
                    var dataArrayFinal = Array();
                    for (i = 0; i < newData.length; i++) {
                        name[i] = newData[i].DistrictName;
                        data[i] = newData[i].ConfirmedCount;
                    }

                    for (j = 0; j < name.length; j++) {
                        var temp = new Array(name[j], data[j]);
                        dataArrayFinal[j] = temp;
                    }
                    Highcharts.chart('container', {
                        chart: {
                            type: 'pie',
                            height: 650,
                            options3d: {
                                enabled: true,
                                alpha: 45
                            }
                        },
                        title: {
                            text: 'Contents of Highsoft\'s weekly fruit delivery'
                        },
                        subtitle: {
                            text: '3D donut in Highcharts'
                        },
                        plotOptions: {
                            pie: {
                                innerSize: 100,
                                depth: 45
                            }
                        },
                        series: [{
                            name: 'Confirmed Count',
                            data: dataArrayFinal,
                            showInLegend: true,
                            dataLabels: {
                                allowOverlap: true,
                                enabled: true,
                                padding: 0,
                                overflow: "none"
                            }
                        }]
                    });
                },
                failure: function (response) {
                    alert(response.d);
                }
            });
        }

        function LoadLineChart() {
            $.ajax({
                type: "POST",
                url: "DrillDownCharts.aspx/GetDistDataForLineCovid",
                contentType: "application/json; charset=utf-8",
                dataType: "json",

                success: function (response) {
                    var Data = response.d;
                    var newData = JSON.parse(response.d)
                    // var labels = [];
                    //var data = [];
                    //var JSData=[];
                    var name = Array();
                    var data = Array();
                    var dataArrayFinal = Array();
                    for (i = 0; i < newData.length; i++) {
                        name[i] = newData[i].DisplayDate;
                        data[i] = newData[i].ConfirmedCount;
                    }

                    for (j = 0; j < name.length; j++) {
                        var temp = new Array(name[j], data[j]);
                        dataArrayFinal[j] = temp;
                    }
                    Highcharts.chart('linecht', {
                        chart: {
                            type: 'line'
                        },
                        title: {
                            text: 'Confirmed Positive Cases'
                        },
                        xAxis: {
                            categories: name
                        },
                        yAxis: {
                            title: {
                                text: ''
                            }
                        },
                        plotOptions: {
                            line: {
                                dataLabels: {
                                    enabled: true
                                },
                                enableMouseTracking: false
                            }
                        },
                        series: [{
                            name: 'Date Wise',
                            data: data
                        }]
                    });
                },
                failure: function (response) {
                    alert(response.d);
                }
            });
        }

