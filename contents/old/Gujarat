        function LoadLineChart5() {
            $.ajax({
                type: "POST",
                url: "DrillDownCharts.aspx/GetDistDataForLineCovidDisrtict",
                contentType: "application/json; charset=utf-8",
                dataType: "json",

                success: function (response) {
                    var Data = response.d;
                    var newData = JSON.parse(response.d)
                    // var labels = [];
                    //var data = [];
                    //var JSData=[];
                    var name = Array();
                    var data = Array();
                    var dist = Array();
                    var Distdata = [];
                    var dataArrayFinal = Array();
                    $.each(newData, function (index, value) {
                        Distdata.push([value["DistrictName"].toString()]);
                        // data.push([value["ConfirmedCount"].toString()]);
                    });

                    for (i = 0; i < newData.length; i++) {
                        name[i] = newData[i].DisplayDate;
                        data[i] = newData[i].ConfirmedCount;
                        dist[i] = newData[i].DistrictName;
                    }

                    for (j = 0; j < name.length; j++) {
                        var temp = new Array(dist[j], data[j]);
                        dataArrayFinal[j] = temp;
                    }
                    var x2 = [];              // Will hold the de-duped result
                    dist.forEach(function (elem) { if (x2.indexOf(elem) === -1) x2.push(elem); });
                    x2;

                    var x1 = [];              // Will hold the de-duped result
                    name.forEach(function (elem) { if (x1.indexOf(elem) === -1) x1.push(elem); });
                    x1;
                    var testarrrray = Array();
                    for (m = 0; m < x1.length; m++) {
                        for (p = 0; p < x2.length; p++) {
                            for (k = 0; k < newData.length; k++) {
                                if (x1[m] == newData[k].DisplayDate && x2[p] == newData[k].DistrictName)
                                    data[k] = newData[k].ConfirmedCount;
                                var temp = new Array(x2[p], data[k]);
                                testarrrray[p] = temp;
                            }
                        }

                    }
                    series = [];
                    series = generateData(data, name, dist);
                    function generateData(cats, names, points) {
                        var ret = {},
                            ps = [],
                            series = [],
                            len = cats.length;

                        //concat to get points
                        for (var i = 0; i < len; i++) {
                            ps[i] = {
                                x: cats[i],
                                y: points[i],
                                n: names[i]
                            };
                        }
                        names = [];
                        //generate series and split points
                        for (i = 0; i < len; i++) {
                            var p = ps[i],
                                sIndex = $.inArray(p.n, names);

                            if (sIndex < 0) {
                                sIndex = names.push(p.n) - 1;
                                series.push({
                                    name: p.n,
                                    data: []
                                });
                            }
                            series[sIndex].data.push(p);
                        }
                        return series;
                    }


                    //console.log(series);
                    Highcharts.chart('linecht5', {
                        chart: {
                            type: 'line'
                        },
                        title: {
                            text: 'Confirmed Case Count'
                        },
                        xAxis: {
                            categories: name
                        },
                        yAxis: {
                            title: {
                                text: ''
                            }
                        },
                        plotOptions: {
                            line: {
                                dataLabels: {
                                    enabled: true
                                },
                                enableMouseTracking: false
                            }
                        },
                        series: series
                        //series: [{
                        //    // name: x2,
                        //    data: series
                        //}]
                    });
                },
                failure: function (response) {
                    alert(response.d);
                }
            });
        }
        // Activate the sliders
        $('#R0').on('change', function () {

            chart2.options.chart.options3d.alpha = this.value;
            showValues();
            chart2.redraw(false);

        });
        $('#R1').on('change', function () {

            chart2.options.chart.options3d.beta = this.value;
            showValues();
            chart2.redraw(false);
        });

        function showValues() {
            $('#R0-value').html(chart2.options.chart.options3d.alpha);
            $('#R1-value').html(chart2.options.chart.options3d.beta);

        }
        showValues();
    </script>
